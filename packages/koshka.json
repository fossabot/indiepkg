{
    "name": "koshka",
    "author": "astrogewgaw",
    "description": "A TUI for searching through catalogs of radio transients: pulsars, FRBs, & RRATs.",
    "url": "https://github.com/astrogewgaw/koshka.git",
    "license": "MIT",
    "bin": {
        "in_source": [
            "koshka"
        ],
        "installed": [
            "koshka"
        ]
    },
    "commands": {
        "all": {
            "install": [
                "go build ."
            ],
            "update": [
                "go build ."
            ]
        }
    },
    "deps": {
        "all": [
            "go"
        ]
    }
}