{
    "name": "onefetch",
    "author": "author",
    "description": "Git repository summary on your terminal.",
    "url": "https://github.com/o2sh/onefetch.git",
    "license": "MIT",
    "bin": {
        "in_source": [
            "onefetch"
        ],
        "installed": [
            "target/release/onefetch"
        ]
    },
    "commands": {
        "all": {
            "install": [
                "cargo build --release"
            ],
            "update": [
                "cargo build --release"
            ]
        }
    },
    "deps": {
        "all": [
            "rustc",
            "cargo"
        ]
    }
}